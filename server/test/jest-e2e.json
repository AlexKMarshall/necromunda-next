{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleDirectories": ["node_modules", "./"],
  "setupFilesAfterEnv": ["./test/setupTests.ts"],
  "collectCoverageFrom": ["./src/**"],
  "coverageDirectory": "./coverage/e2e"
}
